{
    "version": "1.0.8",
    "license": "MIT",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "typings": "dist/index.d.ts",
    "description": "A lightweight state management library for React using RxJS observables.",
    "files": [
        "dist",
        "src"
    ],
    "engines": {
        "node": ">=14"
    },
    "scripts": {
        "build": "node ./build.js",
        "generate:types": "tsc",
        "test": "vitest run",
        "test:watch": "vitest watch",
        "prepare": "node ./build.js",
        "size": "size-limit"
    },
    "peerDependencies": {
        "react": ">=16.8.0 || >= 17 || >= 18",
        "rxjs": ">= 7"
    },
    "prettier": {
        "printWidth": 120,
        "semi": false,
        "singleQuote": true,
        "trailingComma": "es5",
        "tabWidth": 4
    },
    "name": "@guina/manifest",
    "author": "Byron Guina",
    "publishConfig": {
        "access": "public"
    },
    "size-limit": [
        {
            "path": "dist/index.js",
            "limit": "10 KB"
        }
    ],
    "devDependencies": {
        "@size-limit/preset-small-lib": "^7.0.8",
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "^13.0.1",
        "@types/react": "^18.0.4",
        "@types/react-dom": "^18.0.0",
        "@vitejs/plugin-react": "^1.3.1",
        "esbuild": "^0.14.36",
        "esbuild-node-externals": "^1.4.1",
        "happy-dom": "^2.55.0",
        "react": "^18.0.0",
        "react-dom": "^18.0.0",
        "rollup-plugin-analyzer": "^4.0.0",
        "rxjs": "^7.5.5",
        "size-limit": "^7.0.8",
        "tslib": "^2.3.1",
        "typescript": "^4.6.3",
        "vite": "^2.9.4",
        "vitest": "^0.9.3"
    }
}
